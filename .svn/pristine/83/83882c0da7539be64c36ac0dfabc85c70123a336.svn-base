; RES.INI
  ; This sample INI file is used to set resolution.
  +W120 +H100  ; This section has no label.
               ; Select it with "RES"

  [VeryLow]
  +W320 +H200  ; This section has a label.
               ; Select it with "RES[Med]"

  [Low]
  +W400 +H600  ; Labels are not case sensitive.
               ; "RES[high]" works
 Quality=6
 Antialias=off
 Antialias_Depth=5
 Antialias_Threshold=0.03
 Radiosity=on
Display=off
Output_to_File=on
+FN

  [High]
  +W1500 +H1500  ; Labels are not case sensitive.
                 ; "RES[high]" works
 Quality=11
 Antialias=on
 Antialias_Depth=9
 Antialias_Threshold=0.01
 Radiosity=on
Display=on
Output_to_File=on
+FN
Library_Path=/home/cesar/Dropbox/POVRAY
Remove_Bounds=on

  [ReallyHigh]
  +W2000 +H2000  ; Labels are not case sensitive.
                 ; "RES[high]" works
 Quality=10
 Antialias=on
 Antialias_Depth=10
 Antialias_Threshold=0.01
 Radiosity=on
Display=on
Output_to_File=on
+FT
Library_Path=/home/cesar/Dropbox/POVRAY
Remove_Bounds=on

